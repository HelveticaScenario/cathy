{
    "name": "@app/client",
    "private": true,
    "scripts": {
        "build": "echo \"Hello\" && yarn codegen-prod && next build",
        "codegen": "graphql-codegen --config codegen.yml",
        "codegen-prod": "graphql-codegen --config prod.codegen.yml",
        "codegen-watch": "yarn codegen --watch",
        "analyze": "cross-env ANALYZE=true next build",
        "watch": "concurrently --kill-others --names 'CODEGEN,NEXT' --prefix '({name})' --prefix-colors 'yellow.bold,cyan.bold,greenBright.bold' 'yarn codegen-watch' 'yarn next dev'"
    },
    "dependencies": {
        "@apollo/react-common": "^3.1.4",
        "@apollo/react-hooks": "^3.1.5",
        "@apollo/react-ssr": "^3.1.5",
        "@styled-system/css": "^5.1.5",
        "@types/rebass": "^4.0.5",
        "@types/theme-ui": "^0.3.1",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link": "^1.2.14",
        "apollo-link-context": "^1.0.20",
        "apollo-link-error": "^1.1.13",
        "apollo-link-http": "^1.5.17",
        "apollo-link-ws": "^1.0.20",
        "apollo-utilities": "^1.3.3",
        "cookie-session": "^1.4.0",
        "dayjs": "^1.8.24",
        "dotenv": "^8.2.0",
        "firebase": "^7.14.2",
        "firebase-admin": "^8.11.0",
        "graphql": "^14.0.0",
        "graphql-tag": "^2.10.3",
        "isomorphic-unfetch": "^3.0.0",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.15",
        "next": "^9.3.5",
        "next-with-apollo": "^5.0.1",
        "react": "^16.13.1",
        "react-dev": "^0.0.1",
        "react-dom": "^16.13.1",
        "react-firebaseui": "^4.1.0",
        "rebass": "^4.0.7",
        "subscriptions-transport-ws": "^0.9.16",
        "theme-ui": "^0.3.1",
        "ws": "^7.2.5"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^1.13.1",
        "@graphql-codegen/cli": "^1.13.1",
        "@graphql-codegen/typescript": "^1.13.1",
        "@graphql-codegen/typescript-operations": "^1.13.1",
        "@graphql-codegen/typescript-react-apollo": "1.13.1",
        "@next/bundle-analyzer": "^9.3.5",
        "@types/cookie-session": "^2.0.39",
        "@types/js-cookie": "^2.2.6",
        "@types/lodash": "^4.14.150",
        "@types/node": "^13.11.1",
        "@types/react": "^16.9.34",
        "concurrently": "^5.1.0",
        "cross-env": "^7.0.2",
        "typescript": "^3.8.3"
    }
}
